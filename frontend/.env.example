# Frontend Environment Example
# Copy this file to .env.local and fill in your actual values

# Environment Mode
NEXT_PUBLIC_ENV_MODE="LOCAL" # LOCAL, production, or staging

# App URLs
NEXT_PUBLIC_URL="http://localhost:3000"
NEXT_PUBLIC_BACKEND_URL="http://localhost:8000/api"

# Supabase Configuration (Frontend)
NEXT_PUBLIC_SUPABASE_URL="https://your-project.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your_supabase_anon_key_here"

# Authentication (Optional)
NEXT_PUBLIC_GOOGLE_CLIENT_ID="your_google_client_id_here"

# API Keys (Optional)
OPENAI_API_KEY="your_openai_api_key_here"
KORTIX_ADMIN_API_KEY="your_kortix_admin_api_key_here"

# Vercel Edge Config (Optional)
EDGE_CONFIG="https://edge-config.vercel.com/REDACTED?token=REDACTED"

# Feature Flags (Optional)
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_CHAT_HISTORY=true
NEXT_PUBLIC_ENABLE_FILE_UPLOAD=true
